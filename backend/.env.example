# ==== LYX / CoatVision Core – Backend ====

# FastAPI / miljø
ENV=dev
DEBUG=1

# Database (tilpass selv – SQLite/Postgres osv.)
DATABASE_URL=sqlite:///./coatvision_dev.db
# DATABASE_URL=postgresql+psycopg2://user:password@localhost:5432/coatvision

# OpenAI / LYXgpt
OPENAI_API_KEY=sk-XXXX
AGENT_MODEL=gpt-5.1
AGENT_MEMORY_MODEL=gpt-5.1
AGENT_TEMPERATURE=0.4
AGENT_MEMORY_ENABLED=1

# Modeller / analyse
COATVISION_SEG_ONNX_PATH=./models/coatvision_seg_v1.onnx

# Demo-/dashboard-tall (kan overskrives av ekte data)
LYX_ADMIN_TOTAL_JOBS=120
LYX_ADMIN_TOTAL_PANEL_CAPTURES=3400
LYX_ADMIN_WEEKLY_JOBS=18
LYX_ADMIN_WEEKLY_ANALYSES=56
LYX_ADMIN_WEEKLY_QUICKSCANS=89
LYX_ADMIN_WEEKLY_CHAT=42

# Auth / sikkerhet
JWT_SECRET=change-me-super-secret
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS
BACKEND_CORS_ORIGINS=http://localhost:5173,http://localhost:4173
