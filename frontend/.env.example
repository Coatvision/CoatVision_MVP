# Frontend environment example (Vite)
# Copy to frontend/.env and fill values. Do not commit secrets.

# Backend API base URL (Render primary URL)
VITE_API_URL=https://coatvision-mvp-lyxvision.onrender.com

# Supabase client envs (use anon key only)
VITE_SUPABASE_URL=https://<project>.supabase.co
VITE_SUPABASE_ANON_KEY=<anon_key>

# Feature flags (optional)
VITE_COATVISION_USE_REMOTE=true
VITE_ENABLE_DEMO=false
