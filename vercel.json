{
  "version": 2,
  "builds": [
    { "src": "coatvision-app/package.json", "use": "@vercel/static-build" }
  ],
  "env": {
    "VITE_BACKEND_URL": "https://api.coatvision.no",
    "VITE_COATVISION_USE_REMOTE": "1",
    "VITE_SUPABASE_URL": "",
    "VITE_SUPABASE_ANON_KEY": ""
  },
  "buildCommand": "npm ci --prefix coatvision-app && npm run build --prefix coatvision-app",
  "outputDirectory": "coatvision-app/dist"
}
